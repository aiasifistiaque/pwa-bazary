{
	"expo": {
		"name": "Welcome to Bazarey",
		"slug": "bazarey",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/bazareylogo.png",
		"scheme": "bazarey",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/bazareylogo.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff",
			"hideText": true
		},
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.ailabs99.bazarey"
		},
		"android": {
			"adaptiveIcon": {
				"backgroundColor": "#E6F4FE",
				"foregroundImage": "./assets/images/bazareylogo.png"
			},
			"edgeToEdgeEnabled": true,
			"predictiveBackGestureEnabled": false,
			"package": "com.ailabs99.bazarey"
		},
		"web": {
			"output": "static",
			"favicon": "./assets/images/bazareylogo.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/bazareylogo.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff",
					"dark": {
						"image": "./assets/images/bazareylogo.png",
						"backgroundColor": "#000000"
					}
				}
			],
			"expo-secure-store"
		],
		"experiments": {
			"typedRoutes": true,
			"reactCompiler": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "59a5ed8a-c8f9-497e-bffa-9bfea775c171"
			}
		},
		"owner": "ailabs99",
		"updates": {
			"url": "https://u.expo.dev/59a5ed8a-c8f9-497e-bffa-9bfea775c171"
		},
		"runtimeVersion": {
			"policy": "appVersion"
		}
	}
}
